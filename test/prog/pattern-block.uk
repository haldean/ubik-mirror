~ ubik-tests/pattern-block

^ Number
    = Succ Number
    = Zero

: is-it-zero
    ^ Number -> String
    = \n -> ? n {
        . Succ _ => "no"
        . Zero   => "yes"
    }

! emit (concat (concat "should print yes no:\n"
       (concat (is-it-zero Zero)
       (concat " " (is-it-zero (Succ Zero))))) "\n")

