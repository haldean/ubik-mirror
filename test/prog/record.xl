# skip-run

^ Entity
    . id ^ Word
    . description ^ String
    . transform ^ Entity -> Entity


: transform-entity
    ^ Entity -> Entity
    = \x -> Entity id=(. id x)
                   description="transformed"
                   transform=transform-entity

: test-entity
    = Entity id=4 description="a test" transform=transform-entity

!= emit (. description ((. transform test-entity) test-entity))
