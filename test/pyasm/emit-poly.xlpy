# vi: ft=python
from pyasm import *

humanize = load(nid="humanize", uri=uri(name="humanize", scope=pack("native")))
f = const(flt(), t(-1.1252e-6, 0))
w = const(word(), t(351, 0))
apf = apply(func=humanize, arg=f, nid="apply-f", terminal=True)
apw = apply(func=humanize, arg=w, nid="apply-w", terminal=True)

encode([
    graph([humanize, f, w, apw, apf], -1),
])
