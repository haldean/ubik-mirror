# vi: ft=python
from pyasm import *

muri = uri(name="super cool lambda biz", scope=pack("userdef"))

g_ref = graph_ref(uint8(), 1)
s = store(muri, g_ref)
l = load(muri, terminal=True)
g0 = graph([g_ref, s, l], -1)

c0 = const(uint8(), t(7, 10), terminal=True)
g1 = graph([c0], 0)

encode([g0, g1], expect=t(7, 10))
